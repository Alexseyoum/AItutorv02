{
  "name": "tutorbyai",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "prisma generate && next dev --port 3001",
    "dev:turbo": "prisma generate && next dev --turbopack --port 3001",
    "build": "prisma generate && next build",
    "build:turbo": "prisma generate && next build --turbopack",
    "build:analyze": "ANALYZE=true npm run build",
    "start": "next start",
    "start:prod": "NODE_ENV=production next start",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "type-check": "tsc --noEmit",
    "pre-deploy": "npm run lint && npm run type-check && npm run build",
    "deploy-db": "tsx scripts/deploy-db.ts",
    "health-check": "curl -f http://localhost:3000/api/health || exit 1",
    "postbuild": "echo 'âœ… Build completed successfully!'",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset",
    "validate-env": "tsx src/lib/env-validation.ts",
    "vercel-build": "prisma generate && prisma migrate deploy && next build"
  },
  "dependencies": {
    "@huggingface/inference": "^2.8.1",
    "@prisma/client": "^6.17.1",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@vercel/og": "^0.8.5",
    "better-auth": "^1.3.27",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "groq-sdk": "^0.33.0",
    "lucide-react": "^0.544.0",
    "next": "15.5.3",
    "next-themes": "^0.4.6",
    "nodemailer": "^7.0.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/bundle-analyzer": "^15.5.5",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/nodemailer": "^7.0.2",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "prisma": "^6.17.1",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.3.8",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.x"
  }
}
